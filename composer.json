{
    "name": "ipaya/yii2-clipboard",
    "description": "The clipboard extension for Yii 2.",
    "homepage": "https://github.com/iPaya/yii2-clipboard",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "Clipboard"
    ],
    "authors": [
        {
            "name": "Di Zhang",
            "email": "zhangdi_me@163.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/clipboard": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">4.8.20 <6.0"
    },
    "autoload": {
        "psr-4": {
            "iPaya\\Yii2\\Clipboard\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
